/* pages/sellhouse/sell/sell.wxss */

.sell_house_tab {
  width: 100%;
}
.sell_house_tab>.w50 {
  display: inline-block;
  width: 50%；
}
.sell_house_tab .current {
  color: #fd7801;
  border-bottom: 2px solid #fd7801;
}
.arrow_right {
  right: 15px;
}
.sell_inp {
  height: 50px !important;
}
.get_code {
  vertical-align: middle;
  width: 80px;
  height: 36px;
  line-height: 36px;
  padding: 0 !important;
  color: #fd7801;
  margin-top: 7px;
  margin-left: 10px;
  background-color: #fff;
  border: 1px solid #fd7801;
  border-radius: 3px;
}
.entrust {
  margin-top: 20px;
}
.entrust_btn {
  background-color: #fd7801;
  color: #fff;
  border-radius: 3px;
  height: 40px;
  line-height: 40px;
}
button[disabled].entrust_btn {
  background-color: #ccc;
  color: #fff;
}


/* 底部弹出框 */
.bottom_pop {
  width: 100%;
  background-color: #fff;
  bottom: 0px;
  position: fixed;
  left: 0px;
}
.bottom_bar {
  background-color: #f6f6f6;
}

.bottom_col3 {
  width: 33%;
  display: inline-block;
}


.get_code[disabled] {
  border: 1px solid #b9b9b9 !important;
  background-color: #fff !important;
  color: #b9b9b9 !important;
}













/* pages/home/search/search.wxss */
.search_house {
  width: 100%;
  padding: 10px 46px 10px 14px;
  /* position: relative; */
  position: fixed;
  overflow: hidden;
  top: 0px;
  left: 0px;
}
.search_house .house_inp {
  background-color: #fff;
  border: 1px solid #ddd;
  padding-left: 0px;
  padding-right: 36px;
}
.search_house input {
  height: 34px;
  padding-left: 15px;
}
.house_choose {
  position: absolute;
  line-height: 36px;
  /* padding-left: 13px; */
  text-align: center;
  width: 68px;
}
.cancel_btn {
  position: absolute;
  right: 0px;
  top: 0px;
  background-color: transparent;
  border: none;
  padding: 10px;
  line-height: 36px;
}
.bottom_list {
  height: 200px;
}
page {
  height: 100% !important;
}
